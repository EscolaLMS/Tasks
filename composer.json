{
  "name": "escolalms/tasks",
  "description": "Escola Headless LMS Tasks",
  "type": "package",
  "require": {
    "php": "^7.4|^8.0|^8.1",
    "escolalms/auth": "^0",
    "escolalms/core": "^1",
    "escolalms/settings": "^0",
    "laravel/framework": "^8|^9"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": ">=5.0",
    "darkaonline/l5-swagger": "8.1.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Tomasz Smolarek",
      "email": "tomasz.smolarek@escolasoft.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "EscolaLms\\Tasks\\": "src",
      "EscolaLms\\Tasks\\Tests\\": "tests",
      "EscolaLms\\Tasks\\Database\\Factories\\": "database/factories",
      "EscolaLms\\Tasks\\Database\\Seeders\\": "database/seeders"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "EscolaLms\\Tasks\\EscolaLmsTasksServiceProvider"
      ]
    }
  },
  "config": {
    "optimize-autoloader": false,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
